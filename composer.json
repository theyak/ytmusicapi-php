{
	"name": "ytmusicapi/ytmusicapi",
	"description": "Unofficial API for YouTube Music",
	"keywords": [
		"php",
		"youtube",
		"music",
		"youtube music",
		"ytmusicapi"
	],
	"license": "MIT",
	"require": {
		"php": ">=7.4",
		"rmccue/requests": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Ytmusicapi\\": "/src"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"bin": [
		"./src/setup-ytmusicapi"
	],
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"require-dev": {
		"pestphp/pest": "^2.26",
		"pestphp/pest-plugin": "^2.1",
		"pestphp/pest-plugin-type-coverage": "^2.5",
		"laravel/pint": "^1.13",
		"mockery/mockery": "^1.6"
	}
}
